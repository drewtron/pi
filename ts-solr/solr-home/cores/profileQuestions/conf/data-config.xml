<?xml version="1.0" encoding="UTF-8" ?>
<dataConfig>
  <dataSource type="JdbcDataSource" driver="com.mysql.jdbc.Driver" url="jdbc:mysql://10.45.205.242/ark"
              user="glguser" password="password_1"/>
  <document name="ProfileQuestion">
    <entity name="ProfileQuestion"
            query="
              SELECT questionId,
                partyId,
                type,
                questionText,
                subjectTaxonomyPath,
                responseText,
                responseDate
              from profilequestionresponse
              where Response = 'Yes'
              and IsActive = 1">
      <field name="QuestionId" column="QuestionId" />
      <field name="PartyId" column="PartyId" />
      <field name="type" column="id" />
      <field name="questionText" column="id" />
      <field name="subjectTaxonomyPath" column="id" />
      <field name="responseText" column="id" />
      <field name="responseDate" column="id" />

    </entity>
  </document>
</dataConfig>